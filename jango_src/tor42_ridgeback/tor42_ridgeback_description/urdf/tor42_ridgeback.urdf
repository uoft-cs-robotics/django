  <joint name="lift_top_mount_joint" type="fixed">
    <parent link="lift_top_hinge_link"/>
    <child link="lift_top_mount"/>
    <origin rpy="0 0 0" xyz="0 0 0.058"/>
  </joint>
  <!-- Attach the LIDAR & camera mount plate to the tripod head -->
  <link name="lidar_cam_link">
    <visual>
      <geometry>
        <mesh filename="package://tor42_ridgeback_description/meshes/lidar_cam_plate.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.21 0.19 0.036"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.018"/>
    </collision>
  </link>
  <joint name="lidar_cam_joint" type="fixed">
    <parent link="lift_top_mount"/>
    <child link="lidar_cam_link"/>
    <origin rpy="0 0 0" xyz="-0.01 0.0 0.0"/>
  </joint>
  <!-- Create a link for the forward camera mount hole -->
  <link name="front_cam_link"/>
  <joint name="front_cam_joint" type="fixed">
    <parent link="lidar_cam_link"/>
    <child link="front_cam_link"/>
    <origin rpy="0 0 0" xyz="0.09 0 0.0063"/>
  </joint>
  <!-- Create a link for the left camera mount hole -->
  <link name="left_cam_link"/>
  <joint name="left_cam_joint" type="fixed">
    <parent link="lidar_cam_link"/>
    <child link="left_cam_link"/>
    <origin rpy="0 0 1.5708" xyz="-0.017 0.08 0.0063"/>
  </joint>
  <!-- Create a link for the right camera mount hole -->
  <link name="right_cam_link"/>
  <joint name="right_cam_joint" type="fixed">
    <parent link="lidar_cam_link"/>
    <child link="right_cam_link"/>
    <origin rpy="0 0 -1.5708" xyz="-0.017 -0.08 0.0063"/>
  </joint>
  <!-- Attach the Ouster -->
  <joint name="os_sensor_mount_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.036"/>
    <parent link="lidar_cam_link"/>
    <child link="os_sensor_base_link"/>
  </joint>
  <link name="os_sensor_base_link"/>
  <joint name="os_sensor_base_to_baseplate" type="fixed">
    <parent link="os_sensor_base_link"/>
    <child link="os_sensor_baseplate"/>
    <origin xyz="0 0 0.008"/>
  </joint>
  <link name="os_sensor_baseplate">
    <visual>
      <geometry>
        <box size="0.112 0.112 0.016"/>
      </geometry>
      <material name="silver">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.112 0.112 0.016"/>
      </geometry>
    </collision>
  </link>
  <joint name="os_sensor_baseplate_to_body" type="fixed">
    <parent link="os_sensor_baseplate"/>
    <child link="os_sensor"/>
    <origin rpy="0 0 0" xyz="0 0 0.042"/>
  </joint>
  <link name="os_sensor">
    <inertial>
      <mass value="0.33"/>
      <origin rpy="0 0 0" xyz="0 0 0.0365"/>
      <inertia ixx="0.000241148" ixy="0" ixz="0" iyy="0.000241148" iyz="0" izz="0.000264"/>
    </inertial>
    <collision name="base_collision">
      <origin rpy="0 0 0" xyz="0 0 0.0365"/>
      <geometry>
        <cylinder length="0.073" radius="0.04"/>
      </geometry>
    </collision>
    <visual name="base_visual">
      <origin rpy="0 0 1.5707" xyz="0 0 0.0"/>
      <geometry>
        <mesh filename="package://ouster_description/meshes/os1_64.dae"/>
        <!-- <cylinder length="0.073" radius="0.04" /> -->
      </geometry>
    </visual>
  </link>
  <link name="os_imu"/>
  <link name="os_lidar"/>
  <joint name="os_sensor_imu_link_joint" type="fixed">
    <parent link="os_sensor"/>
    <child link="os_imu"/>
    <origin rpy="0 0 0" xyz="0.006253 -0.011775 0.007645"/>
  </joint>
  <gazebo reference="os_imu">
    </gazebo>
  <joint name="os_sensor_lidar_link_joint" type="fixed">
    <parent link="os_sensor"/>
    <child link="os_lidar"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.03618"/>
  </joint>
  <!-- Gazebo requires the ouster_gazebo_plugins package -->
  <gazebo reference="os_sensor">
    <sensor name="os_sensor-OS1-64" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>512</samples>
            <resolution>1</resolution>
            <min_angle>-3.141592653589793</min_angle>
            <max_angle>3.141592653589793</max_angle>
          </horizontal>
          <vertical>
            <samples>64</samples>
            <resolution>1</resolution>
            <min_angle>-0.26</min_angle>
            <max_angle>0.26</max_angle>
          </vertical>
        </scan>
        <range>
          <min>0.9</min>
          <max>75.0</max>
          <resolution>0.03</resolution>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_ouster_laser.so" name="gazebo_ros_laser_controller">
        <topicName>/ouster/points</topicName>
        <frameName>os_lidar</frameName>
        <min_range>0.9</min_range>
        <max_range>75.0</max_range>
        <gaussianNoise>0.008</gaussianNoise>
      </plugin>
    </sensor>
  </gazebo>
  <!-- IMU -->
  <gazebo>
    <plugin filename="libhector_gazebo_ros_imu.so" name="imu_controller">
      <robotNamespace>/</robotNamespace>
      <updateRate>100.0</updateRate>
      <bodyName>os_imu</bodyName>
      <topicName>/ouster/imu</topicName>
      <accelDrift>0.005 0.005 0.005</accelDrift>
      <accelGaussianNoise>0.005 0.005 0.005</accelGaussianNoise>
      <rateDrift>0.005 0.005 0.005 </rateDrift>
      <rateGaussianNoise>0.005 0.005 0.005 </rateGaussianNoise>
      <headingDrift>0.005</headingDrift>
      <headingGaussianNoise>0.005</headingGaussianNoise>
    </plugin>
  </gazebo>
  <!-- Use the ZED macro to create a ZED2 camera URDF -->
  <material name="zed_mat">
    <color rgba="0.8 0.8 0.8 0.8"/>
  </material>
  <material name="zedm_mat">
    <color rgba="0 0 0 0.9"/>
  </material>
  <material name="zed2_mat">
    <color rgba="0.25 0.25 0.25 0.99"/>
  </material>
  <material name="zed2i_mat">
    <color rgba="0.25 0.25 0.25 0.99"/>
  </material>
  <material name="zedx_mat">
    <color rgba="0.25 0.25 0.25 0.99"/>
  </material>
  <material name="zedxm_mat">
    <color rgba="0.25 0.25 0.25 0.99"/>
  </material>
  <!-- Camera mounting point (the threaded screw hole in the bottom) -->
  <link name="zed2i_base_link"/>
  <joint name="zed2i_base_joint" type="fixed">
    <parent link="front_cam_link"/>
    <child link="zed2i_base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
  <!-- Camera Center -->
  <link name="zed2i_camera_center">
    <visual>
      <origin rpy="0 0 0" xyz="-0.01 0 0.0"/>
      <geometry>
        <mesh filename="package://zed_interfaces/meshes/zed2i.stl"/>
      </geometry>
      <material name="zed2i_mat"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://zed_interfaces/meshes/zed2i.stl"/>
      </geometry>
    </collision>
  </link>
  <joint name="zed2i_camera_center_joint" type="fixed">
    <parent link="zed2i_base_link"/>
    <child link="zed2i_camera_center"/>
    <origin rpy="0 0.0 0" xyz="0 0 0.015"/>
  </joint>
  <!-- Left Camera -->
  <link name="zed2i_left_camera_frame"/>
  <joint name="zed2i_left_camera_joint" type="fixed">
    <parent link="zed2i_camera_center"/>
    <child link="zed2i_left_camera_frame"/>
    <origin rpy="0 0 0" xyz="-0.01 0.06 0"/>
  </joint>
  <link name="zed2i_left_camera_optical_frame"/>
  <joint name="zed2i_left_camera_optical_joint" type="fixed">
    <origin rpy="-1.5707963267948966 0.0 -1.5707963267948966" xyz="0 0 0"/>
    <parent link="zed2i_left_camera_frame"/>
    <child link="zed2i_left_camera_optical_frame"/>
  </joint>
  <!-- Right Camera -->
  <link name="zed2i_right_camera_frame"/>
  <joint name="zed2i_right_camera_joint" type="fixed">
    <parent link="zed2i_camera_center"/>
    <child link="zed2i_right_camera_frame"/>
    <origin rpy="0 0 0" xyz="-0.01 -0.06 0"/>
  </joint>
  <link name="zed2i_right_camera_optical_frame"/>
  <joint name="zed2i_right_camera_optical_joint" type="fixed">
    <origin rpy="-1.5707963267948966 0.0 -1.5707963267948966" xyz="0 0 0"/>
    <parent link="zed2i_right_camera_frame"/>
    <child link="zed2i_right_camera_optical_frame"/>
  </joint>
  <!-- Need to also create the parent base link for the camera 
  <link name="front_zed_link" />
 
  <joint name="zed_mount_joint" type="fixed">
    <parent link="front_cam_link" />
    <child link="front_zed_link" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>
-->
</robot>
